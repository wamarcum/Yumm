<%= title("Tables", @table.number) %>
<h2>Table Number: <%= @table.number %></h2>
<h4>Guests: <%= @table.guests %></h4>


<%= link_to "Edit Table", edit_table_path(@table) %>
<%= link_to "Delete Table", table_path(@table),
                            method: :delete,
                            data: { confirm: 
                                    "Are you sure you want to delete this Table?"
                                  }
%>

